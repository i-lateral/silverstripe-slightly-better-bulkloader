{
	"name":"i-lateral/silverstripe-slightly-better-bulkloader",
	"type": "silverstripe-vendormodule",
	"description": "It's the SilverStripe bulk loader, but ever so slightly better",
	"keywords": ["silverstripe", "csv", "import", "bulkload"],
	"license": "BSD-3-Clause",
	"homepage": "https://github.com/i-lateral/silverstripe-slightly-better-bulkloader",
	"authors":[
		{
			"name": "Morven Lewis-Everley",
			"homepage": "https://github.com/mlewis-everley",
			"email" : "morven@ilateral.co.uk",
			"role" : "developer"
		}
	],
	"require":  {
		"silverstripe/framework": "~4.0"
	},
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "silverstripe/versioned": "^1",
        "squizlabs/php_codesniffer": "^3.0"
	},
	"autoload": {
        "psr-4": {
            "ilateral\\SilverStripe\\SlightlyBetterBulkLoader\\": "src",
            "ilateral\\SilverStripe\\SlightlyBetterBulkLoader\\Tests\\": "tests"
        }
    },
    "scripts": {
        "lint": "vendor/bin/phpcs src/ tests/",
        "lint-clean": "vendor/bin/phpcbf src/ tests/"
    },
	"support": {
		"issues": "https://github.com/i-lateral/silverstripe-slightly-better-bulkloader/issues?state=open"
	},
    "minimum-stability": "dev",
    "prefer-stable": true
}
